{
  "name": "lazyfrosch-vagrantenv",
  "version": "0.1.0",
  "author": "Markus Frosch <markus@lazyfrosch.de>",
  "summary": "Make the Vagrant box pretty",
  "license": "GPL-2.0+",
  "source": "https://github.com/lazyfrosch/puppet-vagrantenv.git",
  "project_page": "https://github.com/lazyfrosch/puppet-vagrantenv",
  "issues_url": "https://github.com/lazyfrosch/puppet-vagrantenv/issues",
  "requirements": [
    { "name": "puppet", "version_requirement": ">= 3.8.0 < 7.0.0" }
  ],
  "dependencies": [
    { "name": "puppetlabs/stdlib", "version_requirement": ">= 4.10.0 < 6.0.0" },
    { "name": "puppet/alternatives", "version_requirement": ">= 1.0.0 < 3.0.0" }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "16.04",
        "18.04"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ]
}
